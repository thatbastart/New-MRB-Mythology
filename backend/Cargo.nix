        ];

      };
      "yansi" = rec {
        crateName = "yansi";
        version = "0.5.0";
        edition = "2015";
        sha256 = "0wdx8syhc61lphmgw5cw1vq73isi4szjqriz1k07z19r3r59ziwz";
        authors = [
          "Sergio Benitez <sb@sergio.bz>"
